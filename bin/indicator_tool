#!/usr/bin/env python

import os
import sys
import pprint

current = os.path.dirname(os.path.abspath(__file__))
parent = os.path.dirname(current)
sys.path.append(parent)


from iatidq import models
import iatidq.dqorganisations


_, org = sys.argv

o = models.Organisation.query.filter_by(organisation_code=org.decode('ascii')).first()

ocs = iatidq.dqorganisations._organisation_indicators_split(o, 2)

pprint.pprint(ocs, indent=2)


